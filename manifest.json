{
    "manifest_version": 3,
    "name": "Custom Wallpaper Extension",
    "version": "1.0",
    "description": "Replaces the new tab background with a custom wallpaper.",
    "author": "Your Name",
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  }
  
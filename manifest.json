{
    "manifest_version": 3,
    "name": "Very Cool - newtab",
    "version": "1.0",
    "author": "Mereck McGowan",
    "permissions": ["storage"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "web_accessible_resources": [
        {
            "resources": ["wallpapers/*.jpg"],
            "matches": ["<all_urls>"]
        }
    ],
    "content_scripts": [
        {
            "matches": ["https://www.google.com/search?*"],
            "js": ["script.js"],
            "run_at": "document_end"
        }
    ],
    "icons": {
        "16": "assets/icons/OtherLogo.png",
        "48": "assets/icons/OtherLogo.png",
        "128": "assets/icons/OtherLogo.png"
    }
}

{
    "manifest_version": 3,
    "name": "New Tab Wallpaper",
    "version": "1.0",
    "author": "Mereck McGowan",
    "permissions": ["storage"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
        
    },
    "web_accessible_resources": [
        {
            "resources": ["wallpapers/*.jpg"],
            "matches": ["<all_urls>"]
        }
    ]
}

  
  